(function($) {
  function init() {
    $('.tab[data-tab-id="0"]').addClass('active');
    click_tab();
  }

  function activate_tab(num) {
    $('.tab').removeClass('active');
    $('.column').slideUp();
    $('.tab[data-tab-id="'+num+'"]').addClass('active');
    $('.column[data-column-id="'+num+'"]').slideDown();
  }

  function click_tab() {
    $('.tab').on('click', function() {
      activate_tab($(this).attr('data-tab-id'));
    });
  }

  $(init);
})(jQuery);
(function($) {
  function init() {
  	initialise_filters();
    populate_filters();
  }

  // This function just handles toggling the dropdown menus.

  function initialise_filters() {
  	var news_filter = $('.filters');

  	news_filter.on('click', function(e) {
  		var $this;

  		if ($(e.target).hasClass('filters')) {
  			$this = $(e.target);
  		} else {
  			$this = $(e.target).parents('.filters');
  		}

  		if ($this.hasClass('active')) {
  			$this.removeClass('active');
  			$this.find('.dropdown').slideUp();
  		} else {
  			$this.addClass('active');
  			$this.find('.dropdown').slideDown();
  		}
  	});

  	$('.dropdown li').on('click', click_filter);

  }

  function click_filter(e) {
  	var tax = get_url_vars().category,
  			year = get_url_vars().year,
  			month = get_url_vars().month;

  	if ($(e.target).attr('data-term')) {
  		tax = $(e.target).attr('data-term');
      if (tax === 'all') tax = undefined;
  	} else if ($(e.target).attr('data-date') === 'all') {
      month = undefined;
      year = undefined;
    } else {
  		var link = $(e.target).find('a').context,
  				link_pieces = $(link).attr('href').split('/');

  		month = link_pieces[link_pieces.length - 2];
  		year = link_pieces[link_pieces.length - 3];
  	}

  	if (tax === undefined) {
  		tax = '';
  	} else {
  		tax = 'category='+tax+'&';
  	}

  	if (month === undefined) {
  		month = '';
  	} else {
  		month = 'month='+month;
  	}

  	if (year === undefined) {
  		year = '';
  	} else {
  		year = 'yr='+year+'&';
  	}

  	console.log(tax, year, month);
  	
  	var new_url =  window.location.href.split('?')[0] + '?'+tax+year+month;
  	
  	console.log(new_url);
  	window.location = new_url;

  	return false;
  }

// Read a page's GET URL variables and return them as an associative array.
	function get_url_vars() {
	    var vars = [], hash;
	    var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
	    for(var i = 0; i < hashes.length; i++) {
	        hash = hashes[i].split('=');
	        vars.push(hash[0]);
	        vars[hash[0]] = hash[1];
	    }
	    return vars;
	}

  function populate_filters() {
    var tax = get_url_vars().category,
        year = get_url_vars().year,
        month = get_url_vars().month;

    console.log(tax, year, month);

    if (tax) {
      var tax_nicename = $('li[data-term="'+tax+'"]').text();

      $('.filters.tax li.selected').attr('data-term', tax).text(tax_nicename);
      $('li[data-term="'+tax+'"]').addClass('active');
    }
  }

  $(init);
})(jQuery);
(function($) {
  function init() {
  	setup_gallery();
  }

  function setup_gallery() {
  	// Jump out of method if no gallery.
  	if ($('body').find('.strip.gallery').length < 1) return false;

  	var item = $('.gallery .item'),
  			thumb_item = $('.lightbox-thumb-item');

  	setup_lightbox_galleries();

  	item.on('click', gallery_click);
  	thumb_item.on('click', gallery_click);
  	$('.close-lightbox').on('click', close_gallery);

  }

  function close_gallery() {
  	$('.gallery-lightbox').fadeOut().removeClass('showing');
  }

  function gallery_click(e) {
  	var $this = $(e.target),
  			this_count = $this.attr('data-count');

  	if (!$('.gallery-lightbox').hasClass('showing'))
  		$('.gallery-lightbox').fadeIn().addClass('showing');

  	$('.gallery-lightbox-large').trigger('to.owl.carousel', [this_count, 0, true]);

  }

  function setup_lightbox_galleries() {
  	var options = {
  		items: 1,
  		loop: true,
  		nav: true,
  		dots: false,
  		mouseDrag: false,
  		touchDrag: false
  	},

  	thumb_options = {
  		// items: 8,
  		loop: false,
  		nav: false,
  		dots: false,
  		margin: 10,
  		mouseDrag: true,
  		touchDrag: true,
      autoplay: true,
      autoplayTimeout: 3000,
      autoplaySpeed: 400,
      responsive: {
        0: {
          items: 2
        },
        768: {
          items: 4
        },
        1024: {
          items: 8
        }
      }
  	};

  	$('.gallery-lightbox-large').owlCarousel(options);
  	$('.gallery-lightbox-thumbs').owlCarousel(thumb_options);
  }

	$(init);
})(jQuery);
(function($) {

  var mobile_setup = 0;

  function init() {
    bind_functions();
    search_toggle ();

    $(window).on('resize', function() {
      bind_functions();
    });
  }
  
  function search_toggle() {
	  var search = $('.search-icon'), 
	  	  search_box = $('.search-block');
	  
	  search.on('click', function() {
		 
		 if (search_box.hasClass('open')) {
			 search_box.stop().slideUp();
			 search_box.removeClass('open');
		 } else {
			 search_box.stop().slideDown();
		     search_box.addClass('open');
		     
		     if ($('.more .sub-menu').hasClass('open')) {
			     $('.more').find('.sub-menu').first().stop().slideToggle().toggleClass('open');  
		     } 
		 }
	  });
  }

  function bind_functions() {
    if ($(window).width()  > 1023 ) {
      activate_menu();
      activate_third_level_menu();
      mobile_setup = 0;
    } else {
      mobile_menu();
      mobile_setup = 1;
    }
  }

  function activate_menu() {
    $('.main-navigation').removeClass('mob');
    $('.main-navigation').show();
    $('li.menu-item-has-children').find('.submenu-toggle').remove();
  	var menu_item = $('.main-navigation .menu > li').not('.more');

    $('.menu > li.menu-item-has-children > a').on('click', function(e) {
      if ($(this).parents('li').hasClass('more')) return true;
      e.preventDefault();
      return false;
    });

  	menu_item.hover(function() {
  		$(this).find('.sub-menu').first().stop().slideDown();
  	}, function() {
			$(this).find('.sub-menu').first().stop().slideUp();
  	});

    $('.more').on('click', function() {
      $(this).find('.sub-menu').first().stop().slideToggle().toggleClass('open');
      
      if ($('.search-block').hasClass('open')) {
	      $('.search-block').stop().removeClass('open').slideUp();
      }
    });
  }

  function activate_third_level_menu() {
  	var second_level = $('.sub-menu li.menu-item-has-children');

  	second_level.hover(function() {
  		$(this).find('.sub-menu').first().stop().addClass('active');
  	}, function() {
  		$(this).find('.sub-menu').first().stop().removeClass('active');
  	});
  }

  function mobile_menu() {
    var menu_item = $('.main-navigation .menu > li'),
        appendage = "<div class='submenu-toggle'></div>",
        menu_items_with_children = $('.main-navigation .menu li.menu-item-has-children');

    menu_item.unbind('hover');
    $('.main-navigation').addClass('mob');

    menu_items_with_children.append(appendage);

    $('.submenu-toggle').on('click', function() {
      $(this).siblings('.sub-menu').stop().slideToggle();
      return false;
    });

    menu_items_with_children.on('click', function(e) {
      var this_parent = $(e.target).parent();
      if (this_parent.hasClass('menu-item-has-children')) {

        
        if ($(e.target).parents('.sub-menu').length < 1) {
          $(this).find('.sub-menu').first().stop().slideToggle();
          $(this).parent().toggleClass('active');
          return false;
        }
      }

    });

    $('.hamburger').on('click', function() {
      $('.main-navigation.mob').stop().slideToggle();
      $('.hamburger').toggleClass('active');
    });
  }

	$(init);
})(jQuery);  
(function($) {
  var a_level_year,
      gcse_year;

  function init() {
    initialise_performance_page();
    bind_performance_events();
    initialise_destination();
    bind_destination_events();
    initialise_full_destination_list();
    size_mobile_map();
  }

  function size_mobile_map() {
    var map = $('.map-panel'),
        map_width = map.width(),
        ratio_differentiator = 1.12;

    if ($(window).width() < 768) 
      map.css('height', map_width*ratio_differentiator);

    $(window).on('resize', function() {
      if ($(window).width() > 768) {
        map.css('height', '');
        return false;
      }
      
      map_width = map.width();
      map.css('height', map_width*ratio_differentiator);
    });
  }

  function initialise_full_destination_list() {

    var initial_value = $('select[name="destination-years"]').val();
    $('.tables').hide();
    $('.tables[data-yr="'+initial_value+'"]').show();

    if ($('.tables .table-page').length < 2) {
      return false;
    }
    var options = {
      items: 1,
      loop: false,
      nav: true,
      dots: false,
      mouseDrag: false,
      touchDrag: false
    };

    var tables = $('.tables');

    $.each(tables, function() {
      if ($(this).find('.table-page').length < 2) return;
      if($(window).width()>1024){
        if( !$(this).hasClass('owl-carousel') )$(this).addClass('owl-carousel').owlCarousel(options); 
      }else{
        if($(this).hasClass('owl-carousel'))$(this).removeClass('owl-carousel'); 
      }
    });

    $(window).on('resize', function() {
      $.each(tables, function() {
        if ($(this).find('.table-page').length < 2) return;
        if($(window).width() > 1010){
          if( !($(this).hasClass('owl-carousel') ) ){
            $(this).addClass('owl-carousel').owlCarousel(options); 
          }
        }else{
          $(this).trigger('destroy.owl.carousel');
          $(this).removeClass('owl-carousel');
        }
      });
    });
    // console.log(options,tables);
  }

  function bind_destination_events() {
    $('.london-pin').on('click', switch_map_style);
    $('.uk-pin').on('click', switch_map_style);

    $('.toggle-tab').on('click', toggle_tab_bind);
    $('div[data-function="reload-map"]').on('click', filter_destination_map_bind);
    $('select[name="destination-years"]').on('change', toggle_destination_table);
  }

  function toggle_destination_table() {
    var val = $(this).val();

    $('.tables').hide();
    $('.tables[data-yr="'+val+'"]').show();
  }

  function filter_destination_map_bind() {
    var current_tab = $('.toggle-tab.active').attr('data-function-type'),
        current_value = $('.selects select[name="'+current_tab+'-select"]').val();

    filter_destination_map(current_tab, current_value);
  }

  function filter_destination_map(tab, value) {
    $('.map-pins').hide();

    $('.map-pins[data-type="'+tab+'"][data-value="'+value+'"]').show();
  }

  function toggle_tab_bind() {
    var this_tab = $(this).attr('data-function-type');
    toggle_tab(this_tab);
  }

  function toggle_tab(type) {
    $('.selects select').removeClass('active');
    $('.toggle-tab').removeClass('active');
    $('div.desc').removeClass('active');

    $('.selects select[name="'+type+'-select"]').addClass('active');
    $('div.desc.desc-'+type).addClass('active');
    $('.toggle-tab[data-function-type="'+type+'"]').addClass('active');
  }

  function initialise_destination() {
    // Do some stuff with the dropdowns
    toggle_tab('year');
    filter_destination_map_bind();
    clickable_pins();
  }

  function clickable_pins() {
    $('.map-pin').on("click", function() {
      console.log('mouseentering');
      $(this).trigger('mouseenter');
    });
  }

  function switch_map_style($style) {

    $style = $(this).attr('data-map');

    if ($style === 'uk') {
      $('.map-panel').removeClass('london');
    } else {
      $('.map-panel').addClass('london');
    }
  }

  function initialise_performance_page() {
    set_variables($('.a_level-results').find('.current-item').next().attr('data-year'), $('.gcse-results').find('.current-item').next().attr('data-year'));
    render_graphs();
    toggle_graph_list('graph', 'a_level');
    toggle_graph_list('graph', 'gcse');
  }

  function render_graphs() {

    //Change select boxes
    
    $('.a_level-results').find('.current-item').attr('data-value', a_level_year).text(a_level_year);
    $('.gcse-results').find('.current-item').attr('data-value', gcse_year).text(gcse_year);

    // Show correct graphs

    $('.result-set').hide();
    $('.a_level-results .result-set[data-year="'+a_level_year+'"]').show();
    $('.gcse-results .result-set[data-year="'+gcse_year+'"]').show();
  }

  function set_variables($a_level, $gcse) {
    a_level_year = $a_level;
    gcse_year = $gcse;
  }

  function bind_performance_events() {
    // $('.result-years li.current-item').on('click', open_select_box);
    $('.result-years').on('click', open_select_box); 
    $('.result-years li:not(.current-item)').on('click', change_graph);
    $('.type-toggles div').on('click', toggle_graph_type);
  }

  function change_graph() {
    var new_year = $(this).attr('data-year'),
        level = $(this).attr('data-level');

    if (level === 'a_level') {
      set_variables(new_year, gcse_year);
    } else {
      set_variables(a_level_year, new_year);
    }

    render_graphs();
  }

  function toggle_graph_type() {
    var $this = $(this),
        this_level = $(this).attr('data-level');

    if ($this.hasClass('list-view')) {
      toggle_graph_list('list', this_level);
    } else {
      toggle_graph_list('graph', this_level);
    }
  }

  function toggle_graph_list(style, level) {
    if (!style) {
      console.error('toggle_graph_list didnt have a valid style attribute, setting to "graph"');
      style = 'graph';
    }


    // If we want to toggle both graphs to do the same thing.
    if (!level) {
      $('.result-set').find('ul').hide();
      $('.result-set').find('.result-'+style).show();

      if (style === 'list') {
        $('.type-toggles div').removeClass('active');
        $('.type-toggles .list-view').addClass('active');
      } else if (style === 'graph') {
        $('.type-toggles div').removeClass('active');
        $('.type-toggles .chart-view').addClass('active');
      }
    } else {
      $('.'+level+'-results .result-set').find('ul').hide();
      $('.'+level+'-results .result-set').find('.result-'+style).show();

      if (style === 'list') {
        $('.type-toggles div[data-level="'+level+'"]').removeClass('active');
        $('.type-toggles .list-view[data-level="'+level+'"]').addClass('active');
      } else if (style === 'graph') {
        $('.type-toggles div[data-level="'+level+'"]').removeClass('active');
        $('.type-toggles .chart-view[data-level="'+level+'"]').addClass('active');
      }
    }
  }

  function open_select_box() {
    $(this).toggleClass('open');
  }


  $(init);
})(jQuery);

(function($) {

  // Simply put, this code works by moving the timeline across by the `step_size`
  // variable. So at position 0, the `css:left` of the timeline is 225 * 0 (i.e. 0) for example
  // and at position 1 it would be 225 * 1 (i.e. 225). Along with CSS transitions this will
  // slide the timeline back and forth. We add to the `current_position` variable or subtract
  // from it when we press next or previous.

  var current_position = 0,
      final_slide_number = 0,
      prev_arrow = false,
      next_arrow = false,
      step_size = 275;

  function init() {
    bind_events();
  }

  function bind_events() {
    final_slide_number = $('.timeline-item').last().attr('data-slide');
    check_nav_arrows();

    $('.timeline-next').on('click', function() {
      if (current_position <= parseInt(final_slide_number)+2) {
        return false;
      }

      current_position--;
      move_timeline(current_position);
      return false;
    });

    $('.timeline-prev').on('click', function() {
      if (current_position >= 0) {
        return false;
      }


      current_position++;
      move_timeline(current_position);
      return false;
    });

    $('.timeline-item .after').on('click', function() {
      current_position = $(this).parents('.timeline-item').attr('data-slide');
      move_timeline(current_position);
    });
  }

  function move_timeline(move_amount) {
    check_nav_arrows();
    $('.timeline-items').css('left', step_size*move_amount);
  }

  function check_nav_arrows() {
    $('.timeline-next').addClass('inactive');
    $('.timeline-prev').addClass('inactive');

    if (current_position < 0) {
      prev_arrow = true;
    } else {
      prev_arrow = false;
    }

    if (current_position > parseInt(final_slide_number)+2) {
      next_arrow = true;
    } else {
      next_arrow = false;
    }

    if (next_arrow) $('.timeline-next').removeClass('inactive');
    if (prev_arrow) $('.timeline-prev').removeClass('inactive');
  }

  $(init);
})(jQuery);
